# Copy this file to .env and adjust values for your environment
# .env is git-ignored and automatically loaded by docker-compose

POSTGRES_USER=root
POSTGRES_PASSWORD=root
POSTGRES_DB=bg_log

# Symfony DATABASE_URL (overrides symfony/.env in prod)
# DATABASE_URL=postgresql://your_user:your_password@postgres:5432/bg_log?serverVersion=16&charset=utf8

# Origin allowed for CORS (frontend URL)
CORS_ALLOW_ORIGIN=http://localhost:5173
